{
  "name": "surge",
  "version": "0.0.0",
  "description": "surge web service",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "DEBUG=generate nodemon src/index.js",
    "eslint": "eslint src",
    "eslint-fix": "npm run eslint -- --fix",
    "prettier": "prettier --check src",
    "prettier-write": "prettier --write src",
    "start": "DEBUG=generate node src/index.js",
    "test": "npm run test-only && npm run eslint && npm run prettier",
    "test-only": "vitest run --globals --coverage"
  },
  "eslintConfig": {
    "extends": "cheminfo-react",
    "parser": "babel-eslint",
    "rules": {
      "import/no-unresolved": "off"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cheminfo/surge.git"
  },
  "keywords": [],
  "author": "Luc Patiny",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cheminfo/surge/issues"
  },
  "homepage": "https://github.com/cheminfo/surge#readme",
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@vitest/coverage-v8": "^2.1.8",
    "c8": "^10.1.3",
    "eslint": "^9.17.0",
    "eslint-config-cheminfo": "^13.0.0",
    "nodemon": "^3.1.9",
    "prettier": "^3.4.2",
    "vitest": "^2.1.8"
  },
  "dependencies": {
    "@fastify/cors": "^10.0.2",
    "@fastify/multipart": "^9.0.1",
    "@fastify/sensible": "^6.0.2",
    "@fastify/static": "^8.0.3",
    "@fastify/swagger": "^9.4.0",
    "@fastify/swagger-ui": "^5.2.0",
    "fastify": "^5.2.1",
    "mf-parser": "^3.3.0",
    "openchemlib": "^8.17.0"
  }
}
