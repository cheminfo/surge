{
  "name": "surge",
  "version": "0.0.0",
  "description": "surge web service",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "DEBUG=generate nodemon src/index.js",
    "eslint": "eslint src",
    "eslint-fix": "npm run eslint -- --fix",
    "prettier": "prettier --check src",
    "prettier-write": "prettier --write src",
    "start": "DEBUG=generate node src/index.js",
    "test": "npm run test-only && npm run eslint && npm run prettier",
    "test-only": "vitest run --globals --coverage"
  },
  "eslintConfig": {
    "extends": "cheminfo-react",
    "parser": "babel-eslint",
    "rules": {
      "import/no-unresolved": "off"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cheminfo/surge.git"
  },
  "keywords": [],
  "author": "Luc Patiny",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cheminfo/surge/issues"
  },
  "homepage": "https://github.com/cheminfo/surge#readme",
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@vitest/coverage-v8": "^1.1.0",
    "c8": "^8.0.1",
    "eslint": "^8.56.0",
    "eslint-config-cheminfo": "^9.1.1",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "vitest": "^1.1.0"
  },
  "dependencies": {
    "@fastify/cors": "^8.5.0",
    "@fastify/multipart": "^8.0.0",
    "@fastify/sensible": "^5.5.0",
    "@fastify/static": "^6.12.0",
    "@fastify/swagger": "^8.12.2",
    "@fastify/swagger-ui": "^2.0.1",
    "fastify": "^4.25.2",
    "mf-parser": "^3.1.0",
    "openchemlib": "^8.7.2"
  }
}
